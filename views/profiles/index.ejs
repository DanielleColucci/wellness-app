<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>

<main>
  <div>
    <% profiles.forEach(profile => { %>
      <a href="/profiles/<%= profile._id %>">
        <div>
          <img 
            src="<%= profile.avatar %>" 
            alt="<%= profile.name %>'s avatar"
          >
          <div>
            <h1><%= profile.name %></h1>
          </div>
        </div>
      </a>
    <% }) %>
  </div>
</main>

<%- include('../partials/footer') %>